@{
    ViewBag.Title = "_MainLayout";
}

<html>
<head>
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" type="text/css" href="~/Content/style/reset.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/style/Style.css" />
    <link href='http://fonts.googleapis.com/css?family=Questrial|Noto+Sans:400,700,400italic,700italic|Lato:100,300,400,700,900,100italic,300italic,400italic,700italic,900italic|News+Cycle:400,700|Ubuntu:300,400,500,700,300italic,400italic,500italic,700italic|PT+Serif:400,700,400italic,700italic|Shadows+Into+Light|Droid+Serif:400,700,400italic,700italic|Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800|Raleway:400,100,200,500,300,600,700,800,900|Oxygen:400,700,300|Play:400,700|Indie+Flower|Coming+Soon' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Quicksand:700|Yanone+Kaffeesatz:700,400' rel='stylesheet' type='text/css'>
</head>
<body>
    <nav>
        <div class="outsideNavBox">
            <div class="innerNavBox">
                @Html.ActionLink("Home", "Index", "Home");
                @Html.ActionLink("Calendar", "Announcements", "Home");
                @Html.ActionLink("Profile", "Profile", "Home");
                @Html.ActionLink("About", "AboutUs", "Home");
                @Html.ActionLink("Life", "SchoolLife", "Home");
                @Html.ActionLink("Staff", "Faculty", "Home");
            </div>
        </div>
    </nav>
    <div class="content">
        <img src="~/Content/images/nav-corner.png" class="navCorner"/>
        <div class="pageTitle">
            @RenderSection("pageTitle")
        </div>
        <div class="outsideContentBox">
            <div class="innerContentBox">
                @RenderBody()
            </div>
        </div>
    </div>
</body>
</html>
